# Repository-tracked defaults for GitHub Codespaces bootstrap.
# Secrets must stay in Codespaces secrets, not in git.

CODEX_NPM_PACKAGE="${CODEX_NPM_PACKAGE:-@openai/codex@0.98.0}"
CODEX_DEFAULT_PROFILE="${CODEX_DEFAULT_PROFILE:-openrouter}"
CODEX_MODEL="${CODEX_MODEL:-openai/gpt-5.2-codex}"
CODEX_MODEL_PROVIDER="${CODEX_MODEL_PROVIDER:-openrouter}"
CODEX_REASONING_EFFORT="${CODEX_REASONING_EFFORT:-xhigh}"
CODEX_REASONING_SUMMARY="${CODEX_REASONING_SUMMARY:-none}"
CODEX_APPROVAL_POLICY="${CODEX_APPROVAL_POLICY:-never}"
CODEX_SANDBOX_MODE="${CODEX_SANDBOX_MODE:-danger-full-access}"
CODEX_PROVIDER_NAME="${CODEX_PROVIDER_NAME:-OpenRouter}"
CODEX_PROVIDER_WIRE_API="${CODEX_PROVIDER_WIRE_API:-chat}"
CODEX_PROVIDER_BASE_URL="${CODEX_PROVIDER_BASE_URL:-https://openrouter.ai/api/v1}"

GH_HOST="${GH_HOST:-github.com}"
GH_GIT_PROTOCOL="${GH_GIT_PROTOCOL:-ssh}"
GH_PROMPT="${GH_PROMPT:-disabled}"
GH_TOKEN_VAR="${GH_TOKEN_VAR:-GH_AUTOMATION_TOKEN}"

CODESPACE_GITHUB_ORG="${CODESPACE_GITHUB_ORG:-VilnaCRM-Org}"
